{
    "name": "@esroyo/ahp-lib",
    "version": "0.1.0",
    "exports": "./mod.ts",
    "license": "MIT",
    "tasks": {
        "coverage": "deno task test --clean --reporter=dot --coverage=coverage && deno coverage --lcov --output=coverage/lcov.info --exclude='src/test-utils.ts|vendor' coverage && genhtml -o coverage/report coverage/lcov.info || echo 'No coverage'",
        "fmt": "deno fmt src/",
        "test": "deno test -A --parallel --permit-no-files"
    },
    "lint": {
        "rules": {
            "exclude": ["no-explicit-any"]
        },
        "exclude": [
            "vendor"
        ]
    },
    "fmt": {
        "exclude": [
            "vendor"
        ]
    },
    "imports": {
        "emlapack": "./vendor/npm/emlapack@0.0.3/index.min.cjs",
        "matrix-eig": "./vendor/npm/matrix-eig@1.0.0/index.js",
        "type-fest": "npm:type-fest@4.41.0"
    }
}
